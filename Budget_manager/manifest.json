{
     "manifest_version":3,
     "version":"1.0",
     "name":"Budget Manager",
     "description":"A Budget Manager extension",
     "icons":{
          "128":"money_128.png",
          "48":"money_48.png",
          "16":"money_16.png"
     },
     "action":{                        
          "default_icon":"money_16.png",
          "default_popup":"index.html"
     },
     "options_page":"options.html",
     
     "permissions":[
          "storage",
          "notifications",
          "contextMenus"
     ]
    
}