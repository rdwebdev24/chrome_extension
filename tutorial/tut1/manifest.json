{
     "name":"My YT bookmarks",
     "version":"0.0.1",
     "manifest_version":3,
     "description":"Saving time stamp on you tubr vedios",
     "background":{
          "service_worker":"background.js"
     },
     "action":{
          "default_icon":"icon.png"
     },
     "content_scripts":[
          {
          "matches":["<all_urls>"],
          "js":["content.js"]
          }
     ],
     "permissions":[
          "tabs"
     ]
}